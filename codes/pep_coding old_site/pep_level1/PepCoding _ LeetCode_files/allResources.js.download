<script type="text/javascript">

  function handlePageScrolled() {
    var doc = document.documentElement;
    var top = (window.pageYOffset || doc.scrollTop) - (doc.clientTop || 0);


    if (top >= 100) {
      document.getElementsByClassName('nav-wrapper')[0].classList.add('page-scrolled');
    }
    else {
      document.getElementsByClassName('nav-wrapper')[0].classList.remove('page-scrolled');
    }
  }
  window.onscroll = function (ev) {
    handlePageScrolled();
  };
  window.dataLayer = window.dataLayer || [];
  function gtag() { dataLayer.push(arguments); }
  gtag('js', new Date());
  gtag('config', 'UA-145245404-1');


  $(document).ready(function () {

    setTimeout(twakTo, 1000);
    if (window.history.replaceState) {
      window.history.replaceState(null, null, window.location.href);
    }

    showDeferedData();
    handlePageScrolled();
    aos_init();
    var storyType = document.getElementById('storyType').value;
    if (storyType) {
      ajaxGet('/stories/' + storyType, getStories);
