    for (var i = 0; i < modals.length; i++) {
      M.Modal.init(modals[i]);
    }
  });


  function siteThemeSelect(theme) {
    PEP.createCookie('theme', theme, 365, true);
    document.body.classList.remove('theme1');
    document.body.classList.remove('theme2');
    document.body.classList.remove('theme3');
    document.body.classList.remove('theme4');
    document.body.classList.add(theme);
    saveUserPreferences(theme);
  }

  function showDeferedData() {
    var imgDefer = document.getElementsByTagName('img');
    for (var i = 0; i < imgDefer.length; i++) {
      if (imgDefer[i].getAttribute('data-src')) {
        imgDefer[i].setAttribute('src', imgDefer[i].getAttribute('data-src'));
      }
    }
  }

  function ajaxGet(url, onSuccess) {
    var xmlhttp = new XMLHttpRequest();
    xmlhttp.onreadystatechange = function () {
      if (this.readyState == 4 && this.status == 200) {
        xmlhttp.getResponseHeader("Cache-Control", "public, max-age=2592000");
        var expires = new Date(Date.now() + 259200000).toUTCString();
        xmlhttp.getResponseHeader("Expires", expires);
        onSuccess(this.responseText)
      }
    };
    xmlhttp.open("GET", url, true);
    xmlhttp.send();
  }


  function getStories(data) {
    var myArr = JSON.parse(data);
    document.getElementById("storiesCardSec").parentElement.innerHTML = myArr.data;
  }

  function getCompanies(data) {

    if (document.getElementById("companiesSec")) {
      var myArr = JSON.parse(data);

      document.getElementById("companiesSec").parentElement.innerHTML = myArr.data;
    }
  }


  function openCloseSideNav(open) {
    var mobNav = document.getElementById("mobile-nav");
    var overlay = document.getElementById('sidenavOerlay');
    var target = document.getElementById('mobile-demo');
    if (mobNav.classList.contains('open')) {
      mobNav.classList.remove('open');
      target.classList.remove('show');
      overlay.style = "";
    }
    else {
      mobNav.classList.add('open');
      target.classList.add('show');
      overlay.style.display = "block";
      overlay.style.opacity = "1";
    }

  }



  function showPreview(objFileInput) {
    if (objFileInput.files[0]) {
      var fileReader = new FileReader();
      fileReader.onload = function (e) {
        $('#blah').attr('src', e.target.result);
        $("#targetLayer").html('<img src="' + e.target.result + '" width="200px" height="200px" class="upload-preview circle" />');
      }
      fileReader.readAsDataURL(objFileInput.files[0]);
    }
  }


</script>
