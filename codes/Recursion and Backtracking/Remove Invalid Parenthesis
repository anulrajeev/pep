#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef unsigned long long ull;
typedef vector<int> vi;
typedef vector<vi> vvi;
typedef vector<ll> vll;
typedef vector<vll> vvll;
typedef vector<bool> vb;
typedef vector<vb> vvb;
typedef vector<string> vs;
typedef vector<char> vc;
typedef pair<int, int> pii;

ll mod = 1000000009;

void _main();
int main(){

	_main();
	return 0;
}

int minRem(string s)
{
	stack<char> st;
	int n=s.length();
	for(int i=0;i<n;i++)
		if(s[i]=='(')
			st.push('(');
		else if(st.size() && st.top()=='(')
			st.pop();
		else
			st.push(')');
	return st.size();
}


void remInvalid(string s, int count, unordered_set<string> &taken)
{
	if(taken.count(s)==0)
		taken.insert(s);
	else
		return;

	if(count==0)
	{
		if(minRem(s)==0)
			cout<<s<<'\n';

		
	}
	else 
		for(int i=0;i<s.length();i++)
		{
			string s1=s.substr(0,i);
			string s2=s.substr(i+1,s.length()-i-1);
			remInvalid(s1+s2, count-1, taken);
		}
}

void _main()
{
  	string s;
  	cin>>s;
  	unordered_set<string> taken;
  	remInvalid(s, minRem(s), taken);
}