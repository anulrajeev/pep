#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
typedef unsigned long long ull;
typedef vector<int> vi;
typedef vector<vi> vvi;
typedef vector<ll> vll;
typedef vector<vll> vvll;
typedef vector<bool> vb;
typedef vector<vb> vvb;
typedef vector<string> vs;
typedef vector<char> vc;
typedef pair<int, int> pii;

ll mod = 1000000009;

void _main();
int main(){

	_main();
	return 0;
}

void minDiff(int num[], int index, int n, vi &A, vi &B, vvi &res, int &diff)
{
	if(index==n )
	{
		int a=accumulate(A.begin(), A.end(), 0); // cout<<a<<'\t';
		int b=accumulate(B.begin(), B.end(), 0); //cout<<b<<'\n';
		if(abs(a-b)<diff)
		{
			diff=abs(a-b);
			res[0]=A, res[1]=B;
		}
	}
	else
	{
		if(A.size()<(n+1)/2)
		{
			A.push_back(num[index]);
			minDiff(num, index+1,n,A,B,res,diff);
			A.pop_back();

		}

		if(B.size()<(n+1)/2)
		{
			B.push_back(num[index]);
			minDiff(num, index+1,n,A,B,res,diff);
			B.pop_back();
		}
	}
}

void _main()
{
  	int n;
  	cin>>n;
  	int num[n];
  	for(int i=0;i<n;i++)
  		cin>>num[i];
  	vvi res(2);
  	vi A,B;
  	int diff=INT_MAX;
  	minDiff(num,0,n,A,B,res,diff);
  	// cout<<diff;
  	
  	for(auto v :res)
  	{
  		cout<<'[';
  		for(int i=0;i<v.size()-1;i++)
  			cout<<v[i]<<", ";
  		cout<<v[v.size()-1]<<"] ";
  	}
  	

}