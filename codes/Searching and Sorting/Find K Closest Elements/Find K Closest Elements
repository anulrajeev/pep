vi kClosest(int A[], int n, int k, int x)
{
	priority_queue<pii> pq;
	for(int i=0;i<n;i++)
	{
		int d = abs(A[i]-x);
		if(pq.size()<k)
			pq.push({d,i});
		else if(d<pq.top().first)
		{
			pq.pop();
			pq.push({d,i});
		}
	}
	vi res;
	while(pq.size())
	{
		res.push_back(A[pq.top().second]);
		pq.pop();
	}
	sort(res.begin(), res.end());
	return res;
}

void _main()
{
	int n;
	cin>>n;
	int A[n];
	for(int i=0;i<n;i++)
		cin>>A[i];
	int x,k;
	cin>>k>>x;
	vector<int> v = kClosest(A,n,k,x);
	for(auto x : v)
		cout<<x<<' ';
}

